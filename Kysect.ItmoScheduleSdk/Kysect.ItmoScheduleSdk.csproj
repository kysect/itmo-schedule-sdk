<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <AssemblyName>Kysect.ItmoScheduleSdk</AssemblyName>
    <RootNamespace>Kysect.ItmoScheduleSdk</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Fredi Kats</Authors>
    <Description>ITMO schedule API implementation for C#</Description>
    <RepositoryUrl>https://github.com/kysect/itmo-schedule-sdk</RepositoryUrl>
    <PackageProjectUrl>https://github.com/kysect/itmo-schedule-sdk</PackageProjectUrl>
    <PackageLicenseExpression></PackageLicenseExpression>
    <PackageLicenseUrl>https://github.com/InRedikaWB/ItmoApiWrapper/blob/master/LICENSE</PackageLicenseUrl>
    <Version>1.0.4</Version>
    <Company>Kysect</Company>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Refit" Version="4.6.107" />
  </ItemGroup>

</Project>
